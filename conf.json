{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": ["src"],                    
    "exclude": ["src/index.js"],           
    "includePattern": ".+\\.(js|es)$"      
  },
  "plugins": ["plugins/markdown"],          
  "recurse": true,
  "markdown": {
    "tags": ["file"],                      
    "excludeTags": ["author"],              
    "parser": "gfm",                        
    "hardwrap": true                        
  },
  "templates": {                            
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
      "outputSourceFiles" : true,
      "includeDate": false
		},
		"applicationName": "Demo",
		"disqus": "",
		"googleAnalytics": "",
		"openGraph": {
			"title": "",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "",
			"description": "",
			"keyword": ""
		},
    "outputSourceFiles": false,              
    "outputSourcePath": true,               
    "collapseSymbols": false,               
    "inverseNav": false,                    
    "protocol": "html://",                  
    "methodHeadingReturns": true     
  },
  "opts": {
    "template": "../jsdoc-template",
    "encoding": "utf8",               
    "destination": "./docs/",         
    "recurse": true                  
  }
}
